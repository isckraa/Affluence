<form method="post" action="{{ path('file_attente_delete', {'id': file_attente.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ file_attente.id) }}">
    <button class="btn">Delete</button>
</form>
